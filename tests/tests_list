  sleep 0.3
  echo 'another test string'
  sleep 0.3
  echo 'EOF'
[929-END]

[930]
NAME="Advanced redirections"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls -l > .outputfile'
  echo 'cat < .outputfile > .outputfile2'
  echo 'cat .outputfile2'
  echo 'rm .outputfile .outputfile2'
[930-END]

[931]
