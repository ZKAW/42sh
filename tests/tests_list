[926]
NAME="Simple redirection - >"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls -l > .outputfile'
  echo 'cat .outputfile'
  echo 'rm .outputfile'
[926-END]

[927]
NAME="Simple redirection - >>"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls -l >> .outputfile'
  echo 'pwd >> .outputfile'
  echo 'ls -l /usr >> .outputfile'
  echo 'cat .outputfile'
  echo 'rm .outputfile'
[927-END]

[928]
NAME="Simple redirection - <"
