  echo 'cat .outputfile'
  echo 'rm .outputfile'
[933-END]

[934]
NAME="Pipe with builtin"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN="PATH=$OLDPATH"
TESTS=
  echo 'setenv | /usr/bin/grep PLOP | /usr/bin/sed s/PLOP/YEAH/g'
  echo '/bin/ls | cd rsc'
  echo '/bin/ls -l'
  echo 'exit 45 | /usr/bin/cat /etc/resolv.conf'
[934-END]

